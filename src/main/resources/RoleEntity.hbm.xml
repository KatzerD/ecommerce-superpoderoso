<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.shop.ecommercesuperpoderoso.models.RoleEntity" table="role" schema="public"
           catalog="e-commerce-superpoderoso">
        <id name="id">
            <column name="id" sql-type="integer"/>
        </id>
        <property name="rolName">
            <column name="rol_name" sql-type="varchar" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>